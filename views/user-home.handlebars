<div class="container">
	<div class="row">
		<div class="col s12 m4">
			<div class="user-wrapper wrappers">
				<img src="/uploads/images/{{info.basicInfo.image}}" class="responsive-img circle userImg">
				<h2 class="center-align userTitle"> {{info.basicInfo.first_name}} {{info.basicInfo.last_name}}</h2>
				<div id="user_name" value={{info.basicInfo.user_name}}></div>
				<p class="userDescription center-align">{{info.basicInfo.description}}</p>
				<div class="row">
					<a href="#" data-activates="slide-out" class="button-collapse btn menuBtn"><i class="material-icons medium">menu</i></a>
				</div>
			</div>
			{{!--=======USER ACTIVITY=======--}}
			<div class="user-activity-wrapper wrappers">
				<div class="row">
					<div class="col s12">
						<h2 class="center-align">Progress Feed</h2>
						<hr>
					</div>
					<div class="col s12">

						<ul class="collection" id="userActivityInsert"></ul>
					</div>
				</div>
			</div>
			{{!--Chart Wrapper--}}
		</div>
		<div class="col s12 m8 chartContainer">
			<div class="chart-wrapper">
				<div id="user-name" value="{{info.basicInfo.user_name}}"></div>

				<!-- =============COLLAPSIBLE POPOUT===holds individual collapsibles============= -->
				<ul class="collapsible popout" data-collapsible="accordion">
					<!-- =======GOLF======= -->
					<li>
						<div id="golfCollapsible" class="collapsible-header getChartData active" data-activity="golf" value="golf"><i class="material-icons">golf_course</i><span class="fontCollapsible">Golf</span>
							<span class="fontGrowth">Score Growth: &nbsp;<span class="golfTotalScore"></span></span>
						</div>
						<!-- =======BODY for BUTTONS======= -->
						<!-- =======BODY for CHART======= -->
						<div class="collapsible-body chartDiv golfChartDiv">
							<canvas class="chartArea golfCanvas" height="300" width="580"></canvas>
						</div>
					</li>

					<!-- =======GUITAR======= -->
					<li>
						<div id="guitarCollapsible" class="collapsible-header getChartData" data-activity="guitar" value="guitar"><i class="material-icons">music_note</i><span class="fontCollapsible">Guitar</span>
							<span class="fontGrowth">Hours Growth: &nbsp;<span class="guitarTotalScore"></span></span>
						</div>
						<!-- =======BODY for BUTTONS======= -->
						<div class="collapsible-body">
							<div class="row tight-top-spacing">
								<div class="col s6">
									<a class=" btn cummulative waves-effect waves-light" data-activity="guitar"><i class="material-icons">trending_up</i>Cummulative</a>
								</div>
								<div class="col s6">
									<a class=" btn dayCreated waves-effect waves-light" data-activity="guitar"><i class="material-icons">assessment</i>Bar Graph</a>
								</div>
							</div>
						</div>
						<!-- =======BODY for CHART======= -->
						<div class="collapsible-body chartDiv guitarChartDiv">
							<canvas class="chartArea" height="300" width="580"></canvas>
						</div>
					</li>

					<!-- =======LANG======= -->
					<li>
						<div id="langCollapsible" class="collapsible-header getChartData tertiaryC" data-activity="lang" value="lang"><i class="material-icons">translate</i><span class="fontCollapsible">Espa√±ol</span>
							<span class="fontGrowth">Hours Growth: &nbsp;<span class="langTotalScore"></span></span>

						</div>
						<!-- =======BODY for BUTTONS======= -->
						<div class="collapsible-body">
							<div class="row tight-top-spacing">
								<div class="col s6">
									<a class="btn cummulative waves-effect waves-light" data-activity="lang"><i class="material-icons">trending_up</i>Cummulative</a>
								</div>
								<div class="col s6">
									<a class="btn dayCreated waves-effect waves-light" data-activity="lang"><i class="large material-icons">assessment</i>Bar Graph</a>
								</div>
							</div>
						</div>
						<!-- =======BODY for CHART======= -->
						<div class="collapsible-body chartDiv langChartDiv">
							<canvas class="chartArea" height="300" width="580"></canvas>
						</div>
					</li>
				</ul>
			</div>
		</div>
	</div>
</div>
