{{!--===============ADDING GOLF SKILL MODAL=================--}}
<div id="addGolfSkillsDataModal" class="modal bottom-sheet">
	<div class="modal-content">
		<div class="container">
			<div class="row">
				<div class="col s12">
					<h4 class="center-align">Golf</h4>
					<hr>
					<form class="row">
						<div class="col s6">
							<h5>Years of Experience</h5>
							<input type="hidden" id="user" value="{{info.basicInfo.user_name}}">
							<p class="range-field">
								<input type="range" id="golfYearsExperience" min="0" max="30" />
							</p>
						</div>
						<div class="col s6">
							<h5>How would you rate yourself?</h5>
							<p>
								<input class="with-gap" name="golf-rating" type="radio" id="rating1" value="1" />
								<label for="rating1">Beginner</label>
							</p>
							<p>
								<input class="with-gap" name="golf-rating" type="radio" id="rating2" value="2" />
								<label for="rating2">Experienced</label>
							</p>
							<p>
								<input class="with-gap" name="golf-rating" type="radio" id="rating3" value="3" />
								<label for="rating3">Expert</label>
							</p>
						</div>
					</form>
				</div>
			</div>
		</div>
	</div>
	<div class="modal-footer">
		<button type="submit" id="submitGolfSkillData" class="modal-action modal-close btn btn-large waves-effect waves-light orange">Submit</button>
	</div>
</div>
